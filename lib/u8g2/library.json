{
  "name": "u8g2",
  "version": "1.0.0",
  "description": "A wrapper around U8G2",
  "keywords": "u8g2",
  "repository":
  {
    "type": "git",
    "url": "https://github.com/oligraus/u8g2.git"
  },
  "authors":
  [
  ],
  "license": "BSD",
  "homepage": "https://github.com/oligraus/u8g2.git",
  "frameworks": "",
  "platforms": "native",
  "build": {
    "srcDir": "u8g2/",
    "srcFilter": "+<cppsrc> +<csrc> +<sys/sdl/common>",
    "includeDir": "u8g2/",
    "flags": [
      "-I.",
      "-I./u8g2/cppsrc",
      "-I./u8g2/csrc",
      "-I/usr/include/SDL2 -D_REENTRANT",
      "-lSDL2"
      ]
  }
}
